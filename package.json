{
  "name": "@feelsgoodman/siwviem",
  "version": "1.1.0",
  "description": "Sign-in with Viem",
  "type": "module",
  "main": "dist/index.cjs",
  "types": "dist/index.d.cts",
  "files": [
    "./dist"
  ],
  "scripts": {
    "anvil": "source .env && anvil --fork-url $ANVIL_FORK_URL --chain-id $ANVIL_CHAIN_ID --fork-block-number $ANVIL_BLOCK_NUMBER",
    "deploy:anvil": "forge create test/contracts/ERC1271.sol:ERC1271 --rpc-url http://127.0.0.1:8545 --private-key 0xac0974bec39a17e36ba4a6b4d238ff944bacb478cbed5efcae784d7bf4f2ff80",
    "build": "tsup src/index.ts",
    "changeset:release": "pnpm build && changeset publish",
    "changeset:version": "changeset version && pnpm install --lockfile-only",
    "clean": "rm -rf node_modules && rm -rf dist",
    "lint": "eslint \"src/**/*.ts*\"",
    "lint:fix": "eslint \"src/**/*.ts*\" --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@spruceid/siwe-parser": "^2.0.2",
    "@stablelib/random": "^1.0.1",
    "uri-js": "^4.4.1",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@changesets/changelog-github": "0.4.6",
    "@changesets/cli": "2.25.2",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "@wagmi/cli": "^1.3.0",
    "eslint": "^8.15.0",
    "eslint-config-prettier": "^8.5.0",
    "prettier": "^2.6.2",
    "tsup": "^7.1.0",
    "typescript": "^5.1.6",
    "viem": "^1.4.2",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "viem": "^1.4.2"
  },
  "peerDependenciesMeta": {
    "viem": {
      "optional": false
    }
  },
  "packageManager": "pnpm@8.6.11"
}
