{
  "name": "@feelsgoodman/fs-ui",
  "version": "0.0.0",
  "description": "React component library",
  "repository": {
    "type": "git",
    "url": "https://github.com/spruceid/siwe.git",
    "directory": "packages/frogspawn"
  },
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/plugin",
    "/components",
    "/styles",
    "/dist"
  ],
  "exports": {
    "./plugin": {
      "types": "./dist/plugin/index.d.ts",
      "default": "./dist/plugin/index.js"
    },
    "./components": {
      "types": "./dist/components/index.d.ts",
      "default": "./dist/components/index.js"
    },
    "./components/Anchor": {
      "types": "./dist/components/Anchor/index.d.ts",
      "default": "./dist/components/Anchor/index.js"
    },
    "./components/AppBar": {
      "types": "./dist/components/AppBar/index.d.ts",
      "default": "./dist/components/AppBar/index.js"
    },
    "./components/Box": {
      "types": "./dist/components/Box/index.d.ts",
      "default": "./dist/components/Box/index.js"
    },
    "./components/Button": {
      "types": "./dist/components/Button/index.d.ts",
      "default": "./dist/components/Button/index.js"
    },
    "./components/Code": {
      "types": "./dist/components/Code/index.d.ts",
      "default": "./dist/components/Code/index.js"
    },
    "./components/Flex": {
      "types": "./dist/components/Flex/index.d.ts",
      "default": "./dist/components/Flex/index.js"
    },
    "./components/Grid": {
      "types": "./dist/components/Grid/index.d.ts",
      "default": "./dist/components/Grid/index.js"
    },
    "./components/Heading": {
      "types": "./dist/components/Heading/index.d.ts",
      "default": "./dist/components/Heading/index.js"
    },
    "./components/IconButton": {
      "types": "./dist/components/IconButton/index.d.ts",
      "default": "./dist/components/IconButton/index.js"
    },
    "./components/ScrollArea": {
      "types": "./dist/components/ScrollArea/index.d.ts",
      "default": "./dist/components/ScrollArea/index.js"
    },
    "./components/Text": {
      "types": "./dist/components/Text/index.d.ts",
      "default": "./dist/components/Text/index.js"
    },
    "./package.json": "./package.json",
    "./styles": {
      "default": "./dist/styles/index.css"
    }
  },
  "scripts": {
    "build:css": "postcss ./src/styles/tailwind.css -o ./dist/styles/index.css",
    "build:classes-cache": "npx esbuild src/scripts/classes/index.ts --tsconfig=./tsconfig.json --bundle --outfile=scripts/classes-cache.cjs --platform=node --target=es6",
    "build": "tsup && pnpm cache:update && pnpm build:css",
    "cache:update": "pnpm build:classes-cache && node scripts/classes-cache.cjs",
    "dev": "tsup --watch",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.3",
    "@testing-library/react": "^14.0.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@tsconfig/strictest": "2.0.2",
    "@types/fs-extra": "^11.0.2",
    "@types/node": "^20.4.5",
    "@types/react": "18.2.21",
    "@types/react-dom": "18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "dedent": "^1.5.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "postcss": "^8.4.29",
    "postcss-cli": "^10.1.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "tailwindcss": "^3.3.3",
    "tsup": "^7.1.0",
    "vitest": "^0.33.0"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "typescript": ">=5.0.4"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": false
    },
    "typescript": {
      "optional": true
    }
  },
  "dependencies": {
    "@feelsgoodman/fs-plugin": "workspace:*",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/themes": "^2.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0"
  }
}
