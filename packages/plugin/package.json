{
  "name": "@feelsgoodman/fs-plugin",
  "version": "0.0.0",
  "description": "Tailwind CSS plugin for Frogspawn",
  "repository": {
    "type": "git",
    "url": "https://github.com/spruceid/siwe.git",
    "directory": "packages/plugin"
  },
  "sideEffects": false,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "check": "tsc --pretty --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "dev": "tsup --watch",
    "prepublishOnly": "yarn build",
    "test": "vitest"
  },
  "devDependencies": {
    "@tsconfig/strictest": "2.0.2",
    "@types/node": "^20.4.5",
    "autoprefixer": "^10.4.15",
    "cssnano": "^6.0.1",
    "dedent": "^1.5.1",
    "execa": "^8.0.1",
    "fs-extra": "^11.1.1",
    "jsdom": "^22.1.0",
    "tailwindcss": "^3.3.3",
    "vitest": "^0.33.0",
    "tsup": "^7.1.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.4",
    "tailwindcss": "^3.3.3"
  },
  "peerDependenciesMeta": {
    "typescript": {
      "optional": true
    },
    "tailwindcss": {
      "optional": false
    }
  }
}
